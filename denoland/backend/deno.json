{
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check",
    "dev": "DENO_ENV=development deno run -A ./main.ts",
    "build": "deno run -A ./generate-route.ts"
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "imports": {
    "@oak/oak": "jsr:@oak/oak@^17.2.0",
    "walk": "https://deno.land/std@0.159.0/fs/mod.ts",
    "flags": "https://deno.land/std@0.159.0/flags/mod.ts",
    "crypto": "jsr:@std/crypto",
    "encoding": "jsr:@std/encoding",
    "@utils/": "./utils/",
    "hoshimi-types": "jsr:@hoshimei/hoshimi-types@2.15.1",
    "lodash": "npm:lodash@4.17.21",
    "octokit": "https://esm.sh/octokit@4.0.2",
    "mongodb": "npm:mongodb@7.0.0",
    "sentry": "npm:@sentry/deno@10.32.1",
    "@dev/hoshimi-types": "../../packages/types/denodist/index.ts"
  },
  "exclude": [
    "**/_fresh/*"
  ],
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.asynciterable",
      "dom.iterable",
      "deno.ns"
    ],
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "jsxPrecompileSkipElements": [
      "a",
      "img",
      "source",
      "body",
      "html",
      "head",
      "title",
      "meta",
      "script",
      "link",
      "style",
      "base",
      "noscript",
      "template"
    ],
    "types": [
      "vite/client"
    ]
  }
}