{
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check **/*.ts && deno check **/*.tsx",
    "dev": "DENO_ENV=development deno run -A --watch=static/,routes/ dev.ts",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts",
    "update": "deno run -A -r https://fresh.deno.dev/update ."
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.7.3/",
    "preact": "https://esm.sh/preact@10.19.6",
    "preact/": "https://esm.sh/preact@10.19.6/",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.2.1",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.2.2",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.5.1",
    "@utils/": "./utils/",
    "hoshimi-types": "jsr:@hoshimei/hoshimi-types@2.15.0",
    "lodash": "npm:lodash@4.17.21",
    "xxhashjs": "npm:xxhashjs@0.2.2",
    "octokit": "npm:octokit@4.0.2",
    "mongodb": "https://deno.land/x/mongo@v0.34.0/mod.ts",
    "sentry": "npm:@sentry/deno@9.0.1",
    "@dev/hoshimi-types": "../../packages/types/denodist/index.ts"
  },
  "exclude": [
    "**/_fresh/*"
  ]
}